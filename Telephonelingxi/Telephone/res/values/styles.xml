    <style name="umeng_socialize_edit_padding">
        <item name="android:background">@color/umeng_socialize_list_item_bgcolor</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">fill_parent</item>
        <item name="android:layout_marginLeft">6.0dip</item>
        <item name="android:layout_marginTop">6.0dip</item>
        <item name="android:layout_marginRight">6.0dip</item>
        <item name="android:layout_marginBottom">6.0dip</item>
    </style>
    <style name="Common" />
    <style name="Common.TextTitle" parent="@style/Common">
        <item name="android:textSize">@dimen/common_text_size_title</item>
    </style>
    <style name="Common.TextContent" parent="@style/Common">
        <item name="android:textSize">@dimen/common_text_size_content</item>
    </style>
    <style name="Common.TextWhen" parent="@style/Common">
        <item name="android:textSize">@dimen/common_text_size_when</item>
        <item name="android:textColor">@color/common_text_color_when</item>
    </style>
    <style name="Theme.ActivitySwitch" parent="@android:style/Theme.Translucent.NoTitleBar">
        <item name="android:windowAnimationStyle">@style/ActivitySwitchTheme</item>
    </style>
    <style name="ContentOverlay" parent="@android:style/Theme.Translucent.NoTitleBar">
        <item name="android:windowBackground">@color/white</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:windowFullscreen">true</item>
    </style>
    <style name="tab_text">
        <item name="android:textSize">@dimen/textsize10</item>
        <item name="android:textColor">@drawable/sl_color_b_w</item>
        <item name="android:gravity">center</item>
